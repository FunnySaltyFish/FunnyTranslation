<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle" >

    <!--设置边距-->
    <padding
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp" />
    <!-- 设置圆角矩形 -->
    <corners android:radius="6dp" />

    <!--设置边框粗细和颜色-->
    <stroke
        android:width="2px"
        android:color="#FF9f9f9f" />

    <!--<solid android:color="#838B8B" />-->

</shape>
